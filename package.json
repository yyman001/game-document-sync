    "blob-to-buffer": "^1.2.9",
    "dexie": "^3.2.2",
    "dexie-export-import": "^1.0.3",
    "eslint": "^8.0.1",
    "eslint-config-standard": "^17.0.0",
    "fs-extra": "^10.1.0",
